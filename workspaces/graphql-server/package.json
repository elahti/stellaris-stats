{
  "dependencies": {
    "@apollo/server": "5.1.0",
    "@stellaris-stats/shared": "1.0.0",
    "graphql": "16.12.0",
    "graphql-scalars": "1.25.0",
    "pg": "8.16.3",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@eddeee888/gcg-typescript-resolver-files": "0.14.0",
    "@graphql-codegen/cli": "6.0.2",
    "@types/pg": "8.15.6"
  },
  "name": "@stellaris-stats/graphql-server",
  "scripts": {
    "build": "npm run graphql-server:codegen && npm run clean && npm run build:node",
    "build:node": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "graphql-server:codegen": "graphql-codegen --config codegen.yml && prettier -w src/generated",
    "graphql-server:run": "npx tsx watch src/graphqlServerMain.ts"
  },
  "type": "module",
  "version": "1.0.0"
}