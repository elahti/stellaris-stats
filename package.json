{
  "dependencies": {
    "@apollo/server": "5.2.0",
    "@apollo/server-plugin-response-cache": "5.0.0",
    "@apollo/utils.keyvaluecache": "4.0.0",
    "commander": "14.0.2",
    "dataloader": "2.2.3",
    "date-fns": "4.1.0",
    "graphql": "16.12.0",
    "graphql-scalars": "1.25.0",
    "ioredis": "5.8.2",
    "jomini": "0.9.1",
    "node-pg-migrate": "8.0.4",
    "pg": "8.16.3",
    "pino": "10.1.0",
    "yauzl-promise": "4.0.0",
    "zod": "4.3.4"
  },
  "devDependencies": {
    "@eddeee888/gcg-typescript-resolver-files": "0.15.0",
    "@eslint/js": "9.39.2",
    "@graphql-codegen/cli": "6.1.0",
    "@types/bun": "1.3.5",
    "@types/node": "25.0.3",
    "@types/pg": "8.16.0",
    "@types/yauzl-promise": "4.0.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-prettier": "5.5.4",
    "graphql-codegen-typescript-validation-schema": "0.18.1",
    "pino-pretty": "13.1.3",
    "prettier": "3.7.4",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.51.0"
  },
  "name": "stellaris-stats",
  "scripts": {
    "agent:analyze": "cd agent && dotenvx run -f ../.env.stellaris-stats.secrets -- uv run agent analyze",
    "agent:evals": "cd agent && dotenvx run -f ../.env.stellaris-stats -f ../.env.stellaris-stats.evals -f ../.env.stellaris-stats.secrets -- uv run budget-evals",
    "agent:generate-fixture": "cd agent && dotenvx run -f ../.env.stellaris-stats -- uv run python scripts/generate_sql_fixture.py",
    "agent:list-models": "cd agent && uv run agent list-models",
    "agent:list-saves": "cd agent && dotenvx run -f ../.env.stellaris-stats.secrets -- uv run agent list-saves",
    "build": "npm run graphql:codegen && tsc -p tsconfig.src.json && tsc -p tsconfig.tests.json",
    "db:connect": "dotenvx run -f .env.stellaris-stats --strict -- sh -c 'PGPASSWORD=\"$STELLARIS_STATS_DB_PASSWORD\" psql -h \"$STELLARIS_STATS_DB_HOST\" -p \"$STELLARIS_STATS_DB_PORT\" -U \"$STELLARIS_STATS_DB_USER\" \"$STELLARIS_STATS_DB_NAME\"'",
    "db:dump": "dotenvx run -f .env.stellaris-stats -- sh -c 'PGPASSWORD=$STELLARIS_STATS_DB_PASSWORD pg_dump --clean -h $STELLARIS_STATS_DB_HOST -U $STELLARIS_STATS_DB_USER -d $STELLARIS_STATS_DB_NAME | gzip > /workspace/db-dump-data/db-dump-$(date +%Y-%m-%d-%H-%M-%S).sql.gz'",
    "db:restore": "dotenvx run -f .env.stellaris-stats -- sh -c 'PGPASSWORD=$STELLARIS_STATS_DB_PASSWORD zcat /workspace/db-dump-data/\"$1\" | psql -h $STELLARIS_STATS_DB_HOST -U $STELLARIS_STATS_DB_USER -d $STELLARIS_STATS_DB_NAME' --",
    "db:validate-budget-schema": "dotenvx run -f .env.stellaris-stats --strict -- tsx src/scripts/validateBudgetSchema.ts",
    "format:python": "cd agent && uv run ruff format",
    "gamestateToJson:run": "dotenvx run -f .env.stellaris-stats --strict -- tsx src/scripts/gamestateToJson.ts",
    "graphql-server:run": "dotenvx run -f .env.stellaris-stats --strict -- npx tsx watch --include 'src/graphql/**/*' src/graphql/graphqlServerMain.ts",
    "graphql:codegen": "npm run graphql:codegen:typescript && npm run graphql:codegen:python",
    "graphql:codegen:python": "cd agent && uv run ariadne-codegen",
    "graphql:codegen:typescript": "graphql-codegen --config codegen.yml && prettier -w src/graphql/generated",
    "lint:python": "cd agent && uv run ruff check",
    "lint:typescript": "eslint .",
    "parser:run": "dotenvx run -f .env.stellaris-stats --strict -- npx tsx watch src/parser/parserMain.ts",
    "test:ci:python": "cd agent && uv run pytest -v --tb=short",
    "test:ci:typescript": "dotenvx run -f .env.stellaris-stats.evals -- bun test",
    "test:python": "cd agent && uv run pytest -v",
    "test:typescript": "dotenvx run -f .env.stellaris-stats.evals -- bun test --watch",
    "typecheck:python": "cd agent && uv run pyright"
  }
}